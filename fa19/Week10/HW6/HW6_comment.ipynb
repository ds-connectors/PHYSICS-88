   "source": [
    "# This prints the numbers in the table in https://en.wikipedia.org/wiki/Volume_of_an_n-ball\n",
    "import numpy as np\n",
    "from scipy.special import gamma\n",
    "\n",
    "for n in range(2,11):\n",
    "    coeff=np.pi**(n/2) / gamma(n/2+1)\n",
    "    print(n,coeff)\n"
